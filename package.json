  "main": "index.js",
  "scripts": {
    "test": "npm run test:pre && cross-env NODE_ENV=test nyc mocha tests/mocha.js ./tests/**/*.tests.js",
    "test:watch": "npm run test:pre && cross-env NODE_ENV=test mocha tests/mocha.js ./tests/**/*.tests.js --watch",
    "test:pre": "npm run test:clean && npm run lint:js",
    "test:clean": "rimraf ./coverage && rimraf ./.nyc_output",
    "client:prod:build": "webpack --mode production --config ./build/webpack.client.config.js --colors ./client/app.jsx --output ./dist/client.js",
    "server:prod:build": "webpack --mode production --config ./build/webpack.server.config.js --colors ./index.js --output ./dist/server.js",
    "prod:build": "npm run client:prod:build && npm run server:prod:build",
    "dev:run": "cross-env NODE_ENV=development nodemon -e js --ignore assets/app/ --ignore build/ --ignore client/ --ignore server/data.json --ignore node_modules/ ./build/webpack-dev-server.js",
    "dev:run:no-nodemon": "node ./build/webpack-dev-server.js",
    "prod:run": "node index.dev.js",
    "deploy": "npm run prod:build && cd dist && git add . && git commit -am 'auto deploy' && git push heroku master",
    "deploy:server": "npm run server:prod:build && cd dist && git add . && git commit -am 'auto deploy' && git push heroku master",
    "lint:js": "eslint --ignore-path .gitignore --ignore-pattern build --ignore-pattern webpack ."
  },
  "keywords": [
    "beauty full day",
    "bfd"
  ],
